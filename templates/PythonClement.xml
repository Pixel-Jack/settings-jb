<templateSet group="PythonClement">
  <template name="warn" value="###################### WARNING TEST ###################### " description="Comment a modification for test" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="prv" value="print(f'$VALUE$: {$VALUE$}')&#10;$END$" description="print value with name" toReformat="false" toShortenFQNames="true">
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
  <template name="ppf" value="print(pformat($VALUE$))&#10;$END$" description="Print a pformat" toReformat="false" toShortenFQNames="true">
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="Python" value="true" />
    </context>
  </template>
</templateSet>